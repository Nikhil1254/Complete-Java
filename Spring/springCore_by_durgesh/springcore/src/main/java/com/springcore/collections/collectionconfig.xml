<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context" xmlns:p="http://www.springframework.org/schema/p" xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!--injecting collection properties to object-->
	<bean class="com.springcore.collections.Emp" name="emp1">
		<property name="name" value="Radhe" />
		<property name="phones">
			<list>
				<value>7972490147</value>
				<value>8007278182</value>
				<value>9766976025</value>
				<null /> <!--its used to insert null value-->
			</list>
		</property>
		<property name="addresses">
			<set>
				<value>Delhi</value>
				<value>Jamner</value>
				<value>Jalgaon</value>
				<value>Karmad</value>
			</set>
		</property>
		<property name="courses">
			<map>
				<entry key="Python" value="2Months" />
				<entry key="Cpp" value="1.5Months" />
				<entry key="Java" value="3Months"></entry>
			</map>
		</property>
		<property name="friends">
			<props>
				<prop key="friend1">Kedar</prop>
				<prop key="friend2">Jeet</prop>
				<prop key="friend3">Manjunath</prop>
			</props>
		</property>
	</bean>
</beans>